    let lovDesc = JSON.parse(
      JSON.stringify(store.getState()?.lovDesc?.lovDesc)
    )
    console.log(lovDesc)
    let lauth_key = "";
    let lovDescData: any = {};
    for (let i in lovDesc) {
      if (lovDesc[i].code_desc !== "") {
        lovDescData[lovDesc[i].label + "_a_1_desc"] = lovDesc[i].code_desc;
        lauth_key = lauth_key === "" ? lauth_key + lovDesc[i].label + "_a_1_desc" : lauth_key + ',' + lovDesc[i].label + "_a_1_desc";
      }
    }
    console.log(lovDescData)
    payload["lov_desc"] = {};
    if (stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.AD_1) {
      payload.application["application_status"] = "Success";
      delete payload.application.is_save_to_pega;
      delete payload.application.page_status;
    }
    payload.application["acquisition_channel"] = "D";
    if (stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.AD_1 || stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.ACD_1 || stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.FFD_1) {
      payload["lov_desc"] = lovDescData;
      payload.applicants["lauth_key"] = lauth_key;
      payload.application.source_system_name = "3";
      payload.application.request_id = null;
      payload.application["authentication_flag"] = null;
      //delete armcode//
      delete payload.application.armcode;
      delete payload.application.arm_code;
      delete payload.application.journey_type_hk;
      delete payload.application.is_td_with_casa;
      delete payload.application.notification_required;
      delete payload.application.sub_segment;
      delete payload.application.segment;
      delete payload.application.closing_id;
      delete payload.application.service_type;
      delete payload.application.error;
      delete payload.application.response_type;
      delete payload.application.response_action;
      delete payload.applicants.client_pl_consent4_a_1;
      payload.application["alipay_ref_number"] = null;
      payload.application["form_type"] = null;
      payload.application["application_status"] = null;
      payload.application["stage_wise"] = null;
      payload.application["ext_authentication_reference_number"] = null;
      payload.application["ext_acceptance_reference_number"] = null;
      payload.application["page_wise"] = "yes";
      payload.application["version"] = "";
      payload.applicants["premium_a_1"] = "1258";
      payload.applicants["affiliation_a_1"] = "2132";
      payload.applicants["requested_loan_amount_a_1"] = payload.applicants["requested_loan_amount_a_1"] !== null ? Number(payload.applicants["requested_loan_amount_a_1"]) : null;
      payload.applicants["mgm_referral_code_a_1"] = payload.applicants["mgm_referral_code_a_1"] === "" ? null : payload.applicants["mgm_referral_code_a_1"];
      payload.applicants["card_type_a_1"] = "3268";
      payload.applicants["mgm_flag_a_1"] = "N";
      payload.applicants["product_categories"] = payload.products[0].product_category;
      payload.applicants["res_building_estate_a_1"] = payload.applicants.res_building_estate_pt_a_1;
      payload.applicants["res_city_desc_a_1"] = res_city_desc_a_1[0].code_desc;
      if (stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.AD_1) {
        payload.applicants["bauth_key"] = "last_name_a_1,first_name_a_1,date_of_birth_a_1,mgm_flag_a_1,passport_no_a_1,HKID_a_1,expiry_date_a_1,note_a_1,email_a_1,country_of_issue_a_1,mgm_referral_code_a_1,id type_a_1,mobile_number_a_1,loan_purpose_a_1,requested_loan_tenor_a_1,requested_loan_amount_a_1,res_district_a_1,res_city_a_1,res_country_a_1,res_room_flat_a_1,res_floor_a_1,res_block_a_1,res_name_of_Street_a_1,res_building_estate_a_1";
      }
      if (stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.ACD_1) {
        payload.applicants["currency_pl_a_1"] = "HKD";
        payload.applicants["address_match_flag"] = "Y";
        payload.applicants["off_building_estate_a_1"] = payload.applicants.off_building_estate_pt_a_1;
        payload.applicants["off_city_desc_a_1"] = off_city_desc_a_1[0].code_desc;
        payload.applicants["office_address_a_1"] = resAddressEng(stages.userInput, lovDescSelector, 'office_address')?.replace('\n', '');
        payload.applicants["embossed_name_a_1"] = payload.applicants.last_name_a_1 + ' ' + payload.applicants.first_name_a_1;
        payload.applicants["apply_for_pl_a_1"] = "STANDARD CHARTERED PERSONAL INSTALMENT LOAN";
        payload.applicants["res_address_a_1"] = resAddressEng(stages.userInput, lovDescSelector, 'res_address')?.replace('\n', '');
        payload.applicants["integrated_deposit_account_a_1"] = "";
      }
      payload.applicants["full_name_a_1"] = payload.applicants.last_name_a_1 + ' ' + payload.applicants.first_name_a_1;
      payload.stage["next_page_values"] = "pd-1,bd-1a,bd-1,ad-1,ad-1a,ad-2,ad-3,ad-6,rp,aip-1,doc-3,ld-1,ffd-1";
      payload["dedupeList"] = {};
      payload["client"] = {
        "journey": "prelogin_ntc_or_ntp",
        "auth-type": "manual",
        "login-type": "prelogin"
      }
    }
    payload.stage.stage_status = "incomplete";
    if (stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.FFD_1) {
      payload.stage.workflow_stage_id = 'FFD';
    }
    payload.stage["page_id"] =
      stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.PD_1
        ? ""
        : stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.ACD_1 ? getStageName(CONSTANTS.STAGE_NAMES.RP, stages.stages[0].stageInfo.application.journey_type)
          : getStageName(
            stages.stages[0].stageId,
            stages.stages[0].stageInfo.application.journey_type
          );
    payload.stage["stage_id"] =
      stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.PD_1
        ? ""
        : stages.stages[0].stageId === CONSTANTS.STAGE_NAMES.ACD_1 ? getStageName(CONSTANTS.STAGE_NAMES.RP, stages.stages[0].stageInfo.application.journey_type).split("-")[0].toUpperCase()
          : getStageName(stages.stages[0].stageId, stages.stages[0].stageInfo.application.journey_type).split("-")[0].toUpperCase();
    delete Object.assign(payload, {
      ["fieldmetadata"]: payload["fieldMetaData"],
    })["fieldMetaData"];
    if(stages.stages[0].stageId !== CONSTANTS.STAGE_NAMES.DOC_3 || stages.stages[0].stageId !== CONSTANTS.STAGE_NAMES.LD_1 || stages.stages[0].stageId !== CONSTANTS.STAGE_NAMES.FFD_1 || stages.stages[0].stageId !== CONSTANTS.STAGE_NAMES.FFD_2) {
      payload["applicant_documents"] = [
        {
          applicant_sequence_number: null,
          document_list: null,
          email_id_change: null,
          icdd_reference_no_availability: null,
          journey_type: "NTB",
          last_updated_credit_limit_date_flag: null,
          mobile_no_change: null,
          staff_category: null,
        },
      ];
    }
    payload["oz_templates"] = null;
    payload["preApprovedData"] = {};
    payload.products[0].relation_type_code = "G";
    console.log(payload);
    return sortByAscendingOrder(payload);
  }

}

export default PreApprovalUtil;



